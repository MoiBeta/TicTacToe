<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragments.pvpGameFragment">

    <ImageView
        android:id="@+id/imageView0c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="0"
        app:layout_constraintBottom_toBottomOf="@+id/imageView1c"
        app:layout_constraintEnd_toStartOf="@+id/imageView1c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView1c" />

    <ImageView
        android:id="@+id/imageView1c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="1"
        app:layout_constraintBottom_toTopOf="@+id/imageView4c"
        app:layout_constraintEnd_toStartOf="@+id/imageView2c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView0c"
        app:layout_constraintTop_toBottomOf="@+id/imageViewIcon"
        app:layout_constraintVertical_chainStyle="packed" />

    <ImageView
        android:id="@+id/imageView2c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="2"
        app:layout_constraintBottom_toBottomOf="@+id/imageView1c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView1c"
        app:layout_constraintTop_toTopOf="@+id/imageView1c" />

    <ImageView
        android:id="@+id/imageView3c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="3"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4c"
        app:layout_constraintEnd_toStartOf="@+id/imageView4c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView4c" />

    <ImageView
        android:id="@+id/imageView4c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="24dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="4"
        app:layout_constraintBottom_toTopOf="@+id/imageView7c"
        app:layout_constraintEnd_toStartOf="@+id/imageView5c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView3c"
        app:layout_constraintTop_toBottomOf="@+id/imageView1c" />

    <ImageView
        android:id="@+id/imageView5c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="5"
        app:layout_constraintBottom_toBottomOf="@+id/imageView4c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView4c"
        app:layout_constraintTop_toTopOf="@+id/imageView4c" />

    <ImageView
        android:id="@+id/imageView6c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="6"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7c"
        app:layout_constraintEnd_toStartOf="@+id/imageView7c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView7c" />

    <ImageView
        android:id="@+id/imageView7c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="24dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="7"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView8c"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView6c"
        app:layout_constraintTop_toBottomOf="@+id/imageView4c" />

    <ImageView
        android:id="@+id/imageView8c"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:onClick="casillaSeleccionada"
        android:src="@drawable/ic_empty_square"
        android:tag="8"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7c"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/imageView7c"
        app:layout_constraintTop_toTopOf="@+id/imageView7c" />

    <TextView
        android:id="@+id/textviewPlayerOne"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:tag="9"
        android:text="Player 1"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewIcon"
        app:layout_constraintEnd_toStartOf="@+id/imageViewIcon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageViewIcon" />

    <ImageView
        android:id="@+id/imageViewIcon"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="32dp"
        android:src="@drawable/ic_tic_tac_toe"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textviewPlayerTwo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="Computer"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewIcon"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageViewIcon"
        app:layout_constraintTop_toTopOf="@+id/imageViewIcon" />

</androidx.constraintlayout.widget.ConstraintLayout>